const explodedResults: any[] = [];

this.results.forEach((row: any) => {
  if (row.is_raw_available) {
    explodedResults.push({ ...row, measure: 'Raw' });
  }
  if (row.is_std_available) {
    explodedResults.push({ ...row, measure: 'Std' });
  }
});

// Replace results with exploded version
this.results = explodedResults;

console.log('Results after exploding measures:', this.results);
