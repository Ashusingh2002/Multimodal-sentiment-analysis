Object.entries(filters).forEach(([key, val]) => {
  if (typeof val === 'string') {
    val.split(',').forEach(v => sources[v].add(v));
  } else if (key === 'is_raw_available' && val === true) {
    sources[v].add('Raw');
  } else if (key === 'is_std_available' && val === true) {
    sources[v].add('Std');
  }
});
