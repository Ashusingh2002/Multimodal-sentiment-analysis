refreshAllUniqueAndConstant() {
  // Build apiParams from all fields
  const apiParams: any = {};
  this.fields.forEach(f => {
    if (f.value && f.value.length > 0) {
      const jsonKey = this.fieldToJsonKey[f.name];
      if (jsonKey) {
        apiParams[jsonKey] = f.value;
      }
    }
  });

  // Loop through every field and refresh
  this.fields.forEach(f => {
    this.updateUniqueAndConstantForField(f, apiParams);
  });
}
